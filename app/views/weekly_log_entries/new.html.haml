%h1 Student Engagement Log
=form_for @weekly_log_entry do |f|
  %table
    %tr
      %th Name
      %th Attended Meeting
      %th Engagement
    %tr
      %td= @weekly_log_entry.person.name
      %td=f.check_box :attended_meeting
      %td
        .radio-row
          =f.radio_button :quality, '1', value: 1
          =f.radio_button :quality, '2', value: 2
          =f.radio_button :quality, '3', value: 3
          =f.radio_button :quality, '4', value: 4
          =f.radio_button :quality, '5', value: 5
        .radio-row
          =f.label :quality, 1, value: 1
          =f.label :quality, 2, value: 2
          =f.label :quality, 3, value: 3
          =f.label :quality, 4, value: 4
          =f.label :quality, 5, value: 5
