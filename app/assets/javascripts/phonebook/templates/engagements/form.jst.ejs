<div class="field select-field attendees-field">
  <select multiple id="attendee_ids" name="attendee_ids">
    <option value="Coaching Session">Jim</option>
    <option value="Event">Leonard</option>
    <option value="Course">Nyota</option>
    <option value="Meeting">Scotty</option>
  </select>
  <label for="kind">Attendees</label>
</div>
<div class="field select-field kind-field">
  <select id="kind" name="kind">
    <option value="Coaching Session">Coaching Session</option>
    <option value="Event">Event</option>
    <option value="Course">Course</option>
    <option value="Meeting">Meeting</option>
  </select>
  <label for="kind">Kind</label>
</div>
<div class="field date-field">
  <input name='date' type='date'>
  <label for="date">Date</label>
</div>
<div class="field number-field duration-field">
  <input type="number" name="duration" id="duration" />
  <label for="duration">Duration (Mins)</label>
</div>
<div class="field notes-field file-field">
  <input type="file" id="photo" />
  <label for="duration">Note</label>
</div>
<div class="field longform-field description-field">
  <textarea name='description' placeholder='Description'></textarea>
  <label for="description">Description</label>
</div>
<input type='hidden' name='school_id' />
<div class="field actions-field">
  <input type="submit" value="Done" />
</div>
