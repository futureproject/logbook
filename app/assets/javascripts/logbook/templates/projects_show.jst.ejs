<div class="titlebar">
  <a class="title" href="/logbook/projects">Projects</a>
  <div class="subtitle">&rsaquo; <%= project.name || "Viewing Project" %></div>
  <nav class='titlebar-actions'>
    <a href="/logbook/projects/<%= project.id %>/edit">Edit</a>
  </nav>
</div>
<ul class="factbar">
  <li class="fact">
    <label>status</label>
    <%= project.status %>
  </li>
  <li class="fact">
    <label>created</label>
    <span class="datefact"><%= project.created_at %></span>
  </li>
  <li class="fact">
    <label>Updated</label>
    <span class="datefact"><%= project.updated_at %></span>
  </li>
  <li class="fact">
    <label>School</label>
    <%= project.school_name %>
  </li>
</ul>
<% if (project.description) { %>
<div class="section">
  <pre><%= project.description %></pre>
</div>
<% } %>
</div>
<div class="section notes" id="notes">
  <div class="note" id="new-note">
  </div>
</div>
<div class="section">
  <h2 class="section-header">Project Team - <%= project.people.length %> People</h2>
  <div id="people-table" class="table people-table"></div>
</div>
