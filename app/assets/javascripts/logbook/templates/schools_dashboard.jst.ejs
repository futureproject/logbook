<div class="titlebar">
  <div class="title">Dashboard</div>
  <div class="subtitle">for <%= name %></div>
</div>
<ul class="factbar">
  <li class="fact">
    <label>Dream Director</label>
    <%= dd_name %>
  </li>
  <li class="fact">
    <label>City</label>
    <%= city_name %>
  </li>
</ul>
<div class="section">
  <h2 class="section-header">Stats by School</h2>
  <div id="schools-table" class="table"></div>
</div>
<div class="section">
  <div id="hours_per_person_graph" class="graph">
    <div class="lb-loading"></div>
  </div>
</div>
<div class="section">
  <div id="people_timeline_graph" class="graph">
    <div class="lb-loading"></div>
  </div>
</div>
<div class="section two-col">
  <div id="engagement_percentage_graph" class="graph">
    <div class="lb-loading"></div>
  </div>
  <div id="project_percentage_graph" class="graph">
    <div class="lb-loading"></div>
  </div>
</div>
<div class="section two-col">
  <div id="program_hours_graph" class="graph">
    <div class="lb-loading"></div>
  </div>
  <div id="engagement_counts_graph" class="graph">
    <div class="lb-loading"></div>
  </div>
</div>
<div class="section">
  <div id="weekly_rhythm_graph" class="graph">
    <div class="lb-loading"></div>
  </div>
</div>
<div id="schools-time-filter"></div>
