<div class="titlebar">
  <a class="title" href="/logbook/engagements">Engagements</a>
  <div class="subtitle">&rsaquo; <%= engagement.name || engagement.kind %></div>
  <nav class='titlebar-actions'>
    <a href="/logbook/engagements/<%= engagement.id %>/edit">Edit</a>
  </nav>
</div>
<ul class="factbar">
  <li class="fact">
    <label>kind</label>
    <%= engagement.kind %>
  </li>
  <li class="fact">
    <label>Date</label>
    <%= engagement.date %>
  </li>
  <li class="fact">
    <label>Headcount</label>
    <%= engagement.headcount %>
  </li>
  <li class="fact">
    <label>Duration</label>
    <%= engagement.duration %> Hrs
  </li>
  <li class="fact">
    <label>School</label>
    <%= engagement.school_name %>
  </li>
</ul>
<% if (engagement.description) { %>
<div class="section">
  <pre><%= engagement.description %></pre>
</div>
<% } %>
</div>
<div class="section notes" id="notes">
  <h2 class="section-header">
    <%= engagement.notes.length %> Notes
    <a class='section-header-action' href="/logbook/engagements/<%= engagement.id %>/notes/add" title="Add a note">Add Note <span class='icon-add_circle_outline'></span></a>
  </h2>
  <div id="stickies" class="stickies"></div>
</div>
<div class="section">
  <h2 class="section-header"><%= engagement.headcount %> Attendees</h2>
  <div id="attendees-table" class="table people-table"></div>
</div>
