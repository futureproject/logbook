<form class="list-detail panel">
  <div class="panel-title">
    <h1>Editing Engagement</h1>
    <div class="l-menu"><a class='cancel' href="#cancel"">Cancel</a></div>
    <div class="r-menu">
    <input name="commit" type="submit" value="Done">
    </div>
  </div>
  <div class="panel-body scrollable pad">
    <div class="field">
      <label for="kind">Kind</label>
      <input id="kind" name="kind" type="text">
    </div>
    <div class="field">
      <label for="name">Name</label>
      <input id="name" name="name" type="text">
    </div>
    <div class="field">
      <label for="date">Date</label>
      <input id="date" name="date" type="date">
    </div>
    <div class="field">
      <label for="attendee_ids">Attendees</label>
      <select multiple id="attendee_ids" name="attendee_ids">
      <% for(i=0; i < attendee_ids.length; i++) { %>
        <option value='<%= attendee_ids[i] %>' selected>name</option>
      <% } %>
      </select>
    </div>
    <div class="field">
      <label for="duration">Duration</label>
        <select id="engagement_duration" name="duration">
          <option value="0.25">15 Minutes</option>
          <option value="0.5">30 Minutes</option>
          <option value="1">1 Hour</option>
          <option value="1.5">1.5 Hours</option>
          <option value="2.0">2 Hours</option>
          <option value="3.0">3+ Hours</option>
        </select>
    </div>
    <div class="field">
      <label>Notes</label>
      <textarea name="notes" id="engagement_notes"></textarea>
    </div>
    <hr />
    <div class='button warning'>Delete</div>
  </div><!-- panel-body -->
</form>
