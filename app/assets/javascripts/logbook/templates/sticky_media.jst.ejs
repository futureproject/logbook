<div class="note-date"><%= Date.parse(note.created_at).toString('yyyy-MM-dd') %></div>
<div class="note-user"><%= note.author %></div>
<a title="<%= note.author %>: <%= note.text %>" href="<%= note.poster.original %>" class="thumbnail" style="background-image: url(<%= note.poster.thumbnail %>);">
  <label><%= note.poster.data_content_type %></label>
</a>
<div class="action delete icon-delete"></div>
