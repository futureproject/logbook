    <div class="loading"></div>
  </div>
  <div id="engagement_counts_graph" class="graph">
    <div class="loading"></div>
  </div>
</div>
<div class="section">
  <div id="engagements_context_graph" class="graph">
    <div class="loading"></div>
  </div>
</div>
<div class="section two-col">
  <div id="projects_started_graph" class="graph">
    <div class="loading"></div>
  </div>
  <div id="projects_completed_graph" class="graph">
    <div class="loading"></div>
  </div>
</div>
<div class="section">
  <h2 class="section-header">Stats by City</h2>
  <div id="sites-table" class="table"></div>
</div>
