<form class="list-detail panel">
  <div class="panel-title">
    <h1>New Project</h1>
    <div class="l-menu"><a class='cancel' href="#cancel"">Cancel</a></div>
    <div class="r-menu">
    <input name="commit" type="submit" value="Done">
    </div>
  </div>
  <div class="panel-body scrollable pad">
    <div class="field">
      <label for="name">Name</label>
      <input id="name" name="name" type="text">
    </div>
    <div class="field">
      <label for="leader_ids">Leaders</label>
      <select multiple id="leader_ids" name="leader_ids"></select>
    </div>
    <div class="field">
      <label for="participant_ids">Participants</label>
      <select multiple id="participant_ids" name="participant_ids"></select>
    </div>
  </div><!-- panel-body -->
</form>
